{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "pretty": true,
        "sourceMap": true,
        "target": "es6",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "outDir": "./dist",
        "baseUrl": "./src",
        "lib": [
            "es6"
        ],
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "include": [
        "src/**/*.ts",
        "src/models/quantityModel.ts.bak"
    ],
    "exclude": [
        "node_modules"
    ]
}